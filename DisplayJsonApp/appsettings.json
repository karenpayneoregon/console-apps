{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationName": "Some app",
  "LockMinutes": 10,
  "AzureSettings": {
    "UseAdal": false,
    "AzureGraphVersion": "api-version=1.6",
    "MicrosoftGraphVersion": "1.0"
  },
  "WebPageSettings": {
    "Version": "3.0.0.0",
    "Enabled": false
  },
  "MailSettings": {
    "FromAddress": "FromAddress",
    "Host": "SomeHost",
    "Port": 25,
    "TimeOut": 3000,
    "PickupFolder": "MailDrop"
  },
  "ServicePath": "http://localhost:44340/api/",
  "ClientValidationEnabled": true,
  "UnobtrusiveJavaScriptEnabled": false,
  "ConnectionStrings": {
    "MainDatabaseConnection": "Server=(localdb)\\mssqllocaldb;Database=ComputedSample;Trusted_Connection=True",
    "OtherDatabaseConnection": "Server=SomeOtherServer;Database=SomeDatabase;Persist Security Info=True;Encrypt=False"
  },
  "ConnectionsConfiguration": {
    "ActiveEnvironment": "Development",
    "Development": "Server=(localdb)\\mssqllocaldb;Database=ComputedSample;Trusted_Connection=True",
    "Stage": "Stage connection string goes here",
    "Production": "Prod connection string goes here"
  }
}
